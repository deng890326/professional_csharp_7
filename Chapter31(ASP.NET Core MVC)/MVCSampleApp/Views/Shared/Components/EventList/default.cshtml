@using static MVCSampleApp.ViewComponents.ViewDataKeys;
@model IEnumerable<Event>
@{
}

<h3>@ViewData[EventListDefaultHeader]</h3>
<ul>
    @foreach (Event ev in Model)
    {
        <li>
            <div>@($"{ev.Day:G}")</div>
            <div>@ev.Text</div>
        </li>
    }
</ul>
